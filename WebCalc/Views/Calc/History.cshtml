@model IEnumerable<Domain.Models.History>
@{
    ViewBag.Title = "History";
}
<h2>История операций</h2>
@using (Html.BeginForm("History", "Calc"))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-md-10">
                @Html.TextBox("Search")
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="Filter" class="btn btn-default" />
            </div>
        </div>
    </div>
}

@Html.DisplayForModel("History")

